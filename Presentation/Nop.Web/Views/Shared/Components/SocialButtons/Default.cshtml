@model SocialModel
<ul class="networks">
	@if (!string.IsNullOrEmpty(Model.FacebookLink))
	{
		<li class="facebook"><a href="@Model.FacebookLink" target="_blank">@T("Footer.FollowUs.Facebook")<img src='../Themes/FenchurchThemes/Content/images/facebook.png' /></a></li>
	}
	@if (!string.IsNullOrEmpty(Model.InstagramLink))
	{
		<li class="instagram"><a href="@Model.InstagramLink" target="_blank">@T("Footer.FollowUs.Instagram")<img src='../Themes/FenchurchThemes/Content/images/instagram.png' /></a></li>
	}
	@*@if (Model.NewsEnabled)
	{
		<li class="rss"><a href="@Url.RouteUrl("NewsRSS", new {languageId = Model.WorkingLanguageId})">@T("Footer.FollowUs.RSS")</a></li>
	}
	@if (!string.IsNullOrEmpty(Model.YoutubeLink))
	{
		<li class="youtube"><a href="@Model.YoutubeLink" target="_blank">@T("Footer.FollowUs.Youtube")</a></li>
	}*@
</ul>